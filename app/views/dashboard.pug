extends ./layout.pug

block title
    title= title

block content
    table
        tr
            th #
            th ID
            th Barcode
            th Stock
        tr
        - val = 0
        each data in datas
            td= val++
            td= JSON.stringify(data.ID)
            td= JSON.stringify(data.barCode)
            td= JSON.stringify(data.Stock)
            tr